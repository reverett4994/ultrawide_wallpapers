<% @title= "#{@user.username}'s Submitted Wallpapers" %>
<h2 id="index_header">Wallpapers Submitted by <%=@user.username%></h2>
<% @wallpapers.each do |wallpaper|%>
  <%= render 'wallpapers/gallery',wallpaper: wallpaper %>
<%end%>
<div class="apple_pagination">
  <div class="page_info">
    <%= page_entries_info @wallpapers %>
  </div>
  <%= will_paginate @wallpapers, :container => false %>
</div>
<%=link_to "#{@user.username}'s liked wallpapers","/user/#{@user.username}",class:"link"%>
