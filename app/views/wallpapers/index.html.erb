<p id="notice"><%= notice %></p>

<h1>Listing Wallpapers</h1>


<% @wallpapers.each do |wallpaper| %>
  <%= render 'gallery',wallpaper: wallpaper %>
<% end %>
<br>
<div class="apple_pagination">
  <div class="page_info">
    <%= page_entries_info @wallpapers %>
  </div>
  <%= will_paginate @wallpapers, :container => false %>
</div>
<%= link_to 'New Wallpaper', new_wallpaper_path %>
